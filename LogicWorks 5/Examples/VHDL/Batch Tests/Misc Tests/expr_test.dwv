library IEEE;
use IEEE.std_logic_1164.all;

entity expr_test is

  port (
       In0   : in    std_logic_vector(0 to 1);
       In1   : in    std_logic_vector(0 to 1);
	   Z : out std_logic_vector(0 to 1)
       );

end expr_test;

architecture behavioral of expr_test is

begin

  Z <= In0 and In1 after 2ns;

end behavioral;
